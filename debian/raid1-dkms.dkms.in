PACKAGE_VERSION="@UPSTREAM_VERSION@"
PACKAGE_NAME="raid1"
KERNEL_SHORT_VER="$(echo ${kernelver} | cut -d- -f1)"
CLEAN="make -C /usr/src/linux-headers-${kernelver} M=${dkms_tree}/${PACKAGE_NAME}/${PACKAGE_VERSION}/build/${KERNEL_SHORT_VER} clean"
MAKE="make -C /usr/src/linux-headers-${kernelver} M=${dkms_tree}/${PACKAGE_NAME}/${PACKAGE_VERSION}/build/${KERNEL_SHORT_VER} modules"
AUTOINSTALL="yes"
BUILT_MODULE_LOCATION="${KERNEL_SHORT_VER}"
BUILT_MODULE_NAME="raid1"
DEST_MODULE_LOCATION="/updates/dkms"
